# Letters with macron (in addition to existing sequences in en_US)
# ---------------------------------------------------------------------------- #

# Ӣӣ
<Multi_key> <Cyrillic_I> <macron>		: "Ӣ"	Cyrillic_I_macron # CYRILLIC CAPITAL LETTER I WITH MACRON
<Multi_key> <Cyrillic_i> <macron>		: "ӣ"	Cyrillic_i_macron # CYRILLIC SMALL LETTER I WITH MACRON
<Multi_key> <Cyrillic_I> <underscore>	: "Ӣ"	Cyrillic_I_macron # CYRILLIC CAPITAL LETTER I WITH MACRON
<Multi_key> <Cyrillic_i> <underscore>	: "ӣ"	Cyrillic_i_macron # CYRILLIC SMALL LETTER I WITH MACRON
# Ӯӯ
<Multi_key> <Cyrillic_U> <macron>		: "Ӯ"	Cyrillic_U_macron # CYRILLIC CAPITAL LETTER U WITH MACRON
<Multi_key> <Cyrillic_u> <macron>		: "ӯ"	Cyrillic_u_macron # CYRILLIC SMALL LETTER U WITH MACRON
<Multi_key> <Cyrillic_U> <underscore>	: "Ӯ"	Cyrillic_U_macron # CYRILLIC CAPITAL LETTER U WITH MACRON
<Multi_key> <Cyrillic_u> <underscore>	: "ӯ"	Cyrillic_u_macron # CYRILLIC SMALL LETTER U WITH MACRON

################################################################################
# Letters with combining diacritics
################################################################################

# ---------------------------------------------------------------------------- #
# Letter with combining cedilla
# ---------------------------------------------------------------------------- #

# Г̧г̧
# https://en.wikipedia.org/wiki/Ge_with_cedilla
<dead_cedilla> <Cyrillic_GHE>			: "Г̧"	# CYRILLIC CAPITAL LETTER GHE plus COMBINING CEDILLA
<dead_cedilla> <Cyrillic_ghe>			: "г̧"	# CYRILLIC SMALL LETTER GHE plus COMBINING CEDILLA
<Multi_key> <cedilla> <Cyrillic_GHE>	: "Г̧"	# CYRILLIC CAPITAL LETTER GHE plus COMBINING CEDILLA
<Multi_key> <cedilla> <Cyrillic_ghe>	: "г̧"	# CYRILLIC SMALL LETTER GHE plus COMBINING CEDILLA

