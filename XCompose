# Currency symbols
<dead_currency> <Cyrillic_GHE>			: "₴"	U20B4 # HRYVNIA SIGN
<Multi_key> <equal> <Cyrillic_GHE>		: "₴"	U20B4 # HRYVNIA SIGN
<Multi_key> <Cyrillic_GHE> <equal>		: "₴"	U20B4 # HRYVNIA SIGN
<dead_currency> <Cyrillic_ghe>			: "₴"	U20B4 # HRYVNIA SIGN
<Multi_key> <equal> <Cyrillic_ghe>		: "₴"	U20B4 # HRYVNIA SIGN
<Multi_key> <Cyrillic_ghe> <equal>		: "₴"	U20B4 # HRYVNIA SIGN
<dead_currency> <Cyrillic_ER>			: "₽"	U20BD # RUBLE SIGN
<Multi_key> <equal> <Cyrillic_ER>		: "₽"	U20BD # RUBLE SIGN
<Multi_key> <Cyrillic_ER> <equal>		: "₽"	U20BD # RUBLE SIGN
<dead_currency> <Cyrillic_er>			: "₽"	U20BD # RUBLE SIGN
<Multi_key> <equal> <Cyrillic_er>		: "₽"	U20BD # RUBLE SIGN
<Multi_key> <Cyrillic_er> <equal>		: "₽"	U20BD # RUBLE SIGN
# Tugrik and tenge signs are both based on cyrillic Тт letter.
# en_US.UTF-8 has <dead_currency> <T> for tugrik, so we'll do the same.
<dead_currency> <Cyrillic_TE>			: "₮"	U20AE # TUGRIK SIGN
<Multi_key> <equal> <Cyrillic_TE>		: "₮"	U20AE # TUGRIK SIGN
<Multi_key> <Cyrillic_TE> <equal>		: "₮"	U20AE # TUGRIK SIGN
<dead_currency> <Cyrillic_te>			: "₮"	U20AE # TUGRIK SIGN
<Multi_key> <equal> <Cyrillic_te>		: "₮"	U20AE # TUGRIK SIGN
<Multi_key> <Cyrillic_te> <equal>		: "₮"	U20AE # TUGRIK SIGN
# The following definitions clash with convention of using underscore
# as a mnemonic for macron, but there is no Сс or Тт with macron in cyrillic
<Multi_key> <underscore> <Cyrillic_TE>	: "₸"	U20B8 # TENGE SIGN
<Multi_key> <Cyrillic_TE> <underscore>	: "₸"	U20B8 # TENGE SIGN
<Multi_key> <underscore> <Cyrillic_te>	: "₸"	U20B8 # TENGE SIGN
<Multi_key> <Cyrillic_te> <underscore>	: "₸"	U20B8 # TENGE SIGN
# Som is still unavailable in many fonts, as of 2022
<dead_currency> <Cyrillic_ES>			: "⃀"	U20C0 # SOM SIGN
<Multi_key> <underscore> <Cyrillic_ES>	: "⃀"	U20C0 # SOM SIGN
<Multi_key> <Cyrillic_ES> <underscore>	: "⃀"	U20C0 # SOM SIGN
<dead_currency> <Cyrillic_es>			: "⃀"	U20C0 # SOM SIGN
<Multi_key> <underscore> <Cyrillic_es>	: "⃀"	U20C0 # SOM SIGN
<Multi_key> <Cyrillic_es> <underscore>	: "⃀"	U20C0 # SOM SIGN

